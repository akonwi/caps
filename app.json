{
  "expo": {
    "name": "cap-app",
    "slug": "cap-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-512.png",
    "scheme": "capapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.cap-app",
      "deploymentTarget": "13.4",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to photo library to let you add photos of your hats.",
        "NSCameraUsageDescription": "This app needs access to camera to let you take photos of your hats."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon-512.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "This app needs access to photo library to let you add photos of your hats.",
          "cameraPermission": "This app needs access to camera to let you take photos of your hats."
        }
      ],
      ["expo-document-picker"]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
